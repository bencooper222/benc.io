{
  "name": "benc.io",
  "version": "1.0.0",
  "description": "Personal website of Ben Cooper",
  "keywords": [
    "Personal Website",
    "Ben Cooper",
    "benc.io",
    "Netlify"
  ],
  "bugs": "https://github.com/bencooper222/benc.io/issues",
  "author": {
    "name": "Benjamin Cooper",
    "email": "bencooper222@gmail.com",
    "url": "https://benc.io",
    "encryption": "https://keybase.io/benc222"
  },
  "homepage": "https://benc.io",
  "main": "index.html",
  "repository": "https://github.com/bencooper222/benc.io.git",
  "license": "Unlicense",
  "scripts": {
    "dev": "echo 'Defaulting to cache enabled' && export ENABLE_CACHE=true && parcel index.html",
    "dev:cache": "export ENABLE_CACHE=true && parcel index.html",
    "dev:nocache": "export ENABLE_CACHE=false && parcel index.html",
    "prod": "parcel build index.html -d build/ --public-url ./ && cp keybase.txt security.txt build",
    "sass-lint": "sass-lint --verbose",
    "sass-lint:fix": "sass-lint-auto-fix --verbose",
    "tslint": "tslint -p ./tslint.json --fix",
    "tslint:nofix": "tslint -p ./tslint.json"
  },
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "luxon": "^1.2.0",
    "node-sass": "^4.7.2",
    "parcel-bundler": "^1.7.0",
    "suncalc": "^1.8.0",
    "typescript": "^2.8.3"
  },
  "devDependencies": {
    "@types/luxon": "^0.5.2",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-html": "^4.0.2",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-typescript": "^0.12.0",
    "prettier": "^1.11.1",
    "sass-lint": "^1.12.1",
    "sass-lint-auto-fix": "^0.2.6",
    "tslint": "^5.9.1",
    "tslint-config-airbnb": "^5.8.0",
    "typescript-eslint-parser": "^15.0.0"
  },
  "postinstall": "npm rebuild node-sass",
  "sasslintConfig": ".sass-lint.yml"
}
