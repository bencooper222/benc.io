{
  "name": "benc.io",
  "version": "1.0.0",
  "description": "Personal website of Ben Cooper",
  "keywords": [
    "Personal Website",
    "Ben Cooper",
    "benc.io",
    "Netlify"
  ],
  "engines": {
    "node": ">=8",
    "yarn": ">= 1.5.0"
  },
  "bugs": "https://github.com/bencooper222/benc.io/issues",
  "author": {
    "name": "Benjamin Cooper",
    "email": "bencooper222@gmail.com",
    "url": "https://benc.io",
    "encryption": "https://keybase.io/benc222"
  },
  "homepage": "https://benc.io",
  "main": "index.html",
  "repository": "https://github.com/bencooper222/benc.io.git",
  "license": "UNLICENSED",
  "scripts": {
    "dev": "echo 'Defaulting to cache enabled' && export ENABLE_CACHE=true && cp resources/articles.json resources/articles.use.json && parcel index.html",
    "dev:cache": "export ENABLE_CACHE=true && cp resources/articles.json resources/articles.use.json && parcel index.html",
    "dev:nocache": "export ENABLE_CACHE=false && cp resources/articles.json resources/articles.use.json && parcel index.html",
    "prod": "node bundle.js && parcel build championgg-parser.html -d build && cp report.html text-files/keybase.txt text-files/security.txt build",
    "tslint": "tslint -c tslint.json 'resources/ts/**/*.ts' --fix",
    "tslint:nofix": "tslint -p ./tslint.json",
    "eslint": "eslint bundle.js resources/js --ext .js, --fix",
    "eslint:nofix": "eslint bundle.js resources/js/ --ext .js",
    "sasslint": "prettier resources/css/home.scss --write",
    "sasslint:nofix": "prettier resources/css/home.scss",
    "lint:fix": "yarn eslint && yarn tslint && yarn sasslint"
  },
  "dependencies": {
    "@lvchengbin/sequence": "^1.1.0",
    "bitly": "^5.0.12",
    "luxon": "^1.2.0",
    "node-sass": "^4.7.2",
    "parcel-bundler": "^1.7.0",
    "parcel-plugin-bundle-visualiser": "^1.1.2",
    "suncalc": "^1.8.0",
    "typescript": "^2.8.3"
  },
  "devDependencies": {
    "@types/luxon": "^0.5.2",
    "dotenv": "^5.0.1",
    "eslint": "^4.9.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-html": "^4.0.2",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-prettier": "^2.6.0",
    "prettier": "1.13.5",
    "tslint": "^5.9.1",
    "tslint-config-airbnb": "^5.8.0",
    "tslint-plugin-prettier": "^1.3.0"
  },
  "postinstall": "npm rebuild node-sass",
  "sasslintConfig": ".sass-lint.yml",
  "prettier": {
    "singleQuote": true
  }
}
